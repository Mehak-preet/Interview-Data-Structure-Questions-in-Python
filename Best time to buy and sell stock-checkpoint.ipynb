    "def maxprofit(nums):\n",
    "    ans=0\n",
    "    currsum=0\n",
    "    for i in range(len(nums)-1):\n",
    "        currsum+=nums[i+1]-nums[i]\n",
    "        if currsum<0:\n",
    "            currsum=0\n",
    "        ans=max(ans,currsum)\n",
    "    return ans\n",
    "nums=[7,1,5,3,6,4]\n",
    "maxprofit(nums)\n",
